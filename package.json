{
    "name": "vue3-element-plus-webpack",
    "version": "1.0.10",
    "description": "vue3-element-plus-webpack",
    "author": "huangzheng",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development ENV_CONFIG=dev node build/dev-server.js --mock",
        "start": "npm run dev",
        "build-dev": "cross-env NODE_ENV=production ENV_CONFIG=dev_build node build/build.js",
        "build-test": "cross-env NODE_ENV=production ENV_CONFIG=test node build/build.js",
        "build-demo": "cross-env NODE_ENV=production ENV_CONFIG=demo node build/build.js",
        "build": "cross-env NODE_ENV=production ENV_CONFIG=prod node build/build.js",
        "build-preview": "npm run build --preview && npm run play-dist",
        "play-dist": "node -e \"require('dynamic-mocker').start('./mock/static-config.js')\"",
        "mock": "node -e \"require('dynamic-mocker').start('./mock/mock-config.js')\" ",
        "proxy80": "node -e \"require('dynamic-mocker').start('./mock/mock-proxy80.js')\" ",
        "lint": "eslint --ext .js,.vue src",
        "fixlint": "eslint --fix --ext .js,.vue src mock",
        "build-font": "node -e \"require('./build/svg2font.js').build()\"",
        "show-map": "node -e \"console.log(require('./config').build.sourceMapPath)\"",
        "rmcache": "node -e \"require('rimraf')('./node_modules/.cache', console.log)\""
    },
    "dependencies": {
        "axios": "^1.7.7",
        "date-any": "^1.0.7",
        "echarts": "^5.5.1",
        "element-plus": "^2.8.5",
        "qrcode-vue": "^1.2.0",
        "vue": "^3.5.12",
        "vue-router": "^4.4.5",
        "vue-smooth-dnd": "^0.8.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.3",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.23.3",
        "@babel/preset-stage-2": "^7.8.3",
        "@babel/register": "^7.22.15",
        "@vue/babel-preset-jsx": "^1.4.0",
        "@vue/babel-plugin-jsx": "^1.2.5",
        "autoprefixer": "^10.4.20",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.3.0",
        "chalk": "^5.3.0",
        "connect-history-api-fallback": "^2.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "core-js": "^3.38.1",
        "cross-env": "^7.0.3",
        "cross-spawn": "^7.0.3",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "dynamic-mocker": "^1.2.19",
        "eslint": "^7.25.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-friendly-formatter": "4.0.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-standard": "4.0.2",
        "eslint-plugin-vue": "^7.9.0",
        "eventsource-polyfill": "^0.9.6",
        "express": "^4.21.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.6.0",
        "join-file-content-plugin": "latest",
        "js-conditional-compile-loader": "latest",
        "mini-css-extract-plugin": "^2.9.1",
        "opn": "^6.0.0",
        "ora": "^8.1.0",
        "postcss-loader": "^8.1.1",
        "postcss-pxtorem": "^6.1.0",
        "regenerator-runtime": "^0.14.1",
        "rimraf": "^6.0.1",
        "sass": "^1.45.1",
        "sass-loader": "^14.2.1",
        "shelljs": "^0.8.5",
        "unplugin-vue-components": "^0.27.4",
        "url-loader": "^4.1.1",
        "vue-loader": "^17.4.2",
        "vue-style-loader": "^4.1.3",
        "webpack": "^5.94.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-dev-middleware": "^7.4.2",
        "webpack-hot-middleware": "^2.26.1",
        "webpack-iconfont-plugin-nodejs": "latest",
        "webpack-merge": "^6.0.1",
        "webpack-theme-color-replacer": "^1.5.2"
    },
    "engines": {
        "node": ">= 4.0.0",
        "npm": ">= 3.0.0"
    },
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git"
    },
    "keywords": []
}
