{
    "name": "vue-element-ui-scaffold-webpack4",
    "version": "1.0.9",
    "description": "vue-element-ui-scaffold-webpack4",
    "author": "huangzheng",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development ENV_CONFIG=dev node build/dev-server.js --mock",
        "start": "npm run dev",
        "build-dev": "cross-env NODE_ENV=development ENV_CONFIG=dev_build node build/build.js",
        "build-test": "cross-env NODE_ENV=production ENV_CONFIG=test node build/build.js",
        "build-demo": "cross-env NODE_ENV=production ENV_CONFIG=demo node build/build.js",
        "build": "cross-env NODE_ENV=production ENV_CONFIG=prod node build/build.js",
        "mock": "node ./node_modules/dynamic-mocker/run ./mock/mock-config.js",
        "proxy80": "sudo node ./node_modules/dynamic-mocker/run ./mock/proxy80.js",
        "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "e2e": "cross-env BABEL_ENV=testing NODE_ENV=testing node test/e2e/runner.js",
        "test": "npm run unit && npm run e2e",
        "lint": "eslint --ext .js,.vue src",
        "fixlint": "eslint --fix --ext .js,.vue src",
        "build-font": "node -e \"require('./build/svg2font.js').build()\""
    },
