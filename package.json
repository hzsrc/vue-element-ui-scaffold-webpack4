        "eslint-plugin-standard": "4.0.2",
        "eslint-plugin-vue": "^7.9.0",
        "eventsource-polyfill": "^0.9.6",
        "express": "^4.19.2",
        "file-loader": "^6.2.0",
        "glob": "<=7.*",
        "globby": "^11.0.3",
