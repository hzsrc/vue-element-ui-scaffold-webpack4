        "lodash": "*",
        "qrcode-vue": "^1.2.0",
        "slot-layout": "^1.0.5",
        "vue": "^2.6.12",
        "vue-router": "^3.5.4",
        "vue-smooth-dnd": "^0.8.1",
        "vuex": "^3.6.2"
    },
    "devDependencies": {

        "@babel/core": "^7.23.3",
