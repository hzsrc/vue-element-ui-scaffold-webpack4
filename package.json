        "@vue/babel-preset-jsx": "^1.4.0",
        "autoprefixer": "^10.2.5",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.3.0",
        "babel-plugin-component": "^1.1.1",
        "bluebird": "^3.7.2",
        "chalk": "^4.1.0",
        "copy-dir": "^1.3.0",
        "copy-webpack-plugin": "^8.1.1",
        "core-js": "^3.33.3",
